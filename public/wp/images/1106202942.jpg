<script type="text/javascript">
	var p_osa0040002_004_s_0001_01 = function($el){
		var imgS = "300px",
			imgL = "450px",
			interval = 1000,
			confirmNum = 5,
			wait = 100,	
			imgNum = 0,
			timer,
			count = 0;
		timer = setInterval(function(){
			count++;
			imgNum = $el.find("img").length;
			if(imgNum > 0 && imgNum < 5){
				clearInterval(timer);
				setTimeout(function(){
					imgNum = $el.find("img").length;
					$el.find("li").hide();
					if(imgNum == 1){
						$el.find("img").css('width',imgL).closest("li").css('margin-right',0).show();
					}else if(imgNum == 2){
						$el.find("img").css('width',imgL);
						$el.find("img").each(function(index){
							if(index === 0){
								snm.$(this).closest("li").css({'margin-right':'10px','clear':'none'}).show();
							}else{
								snm.$(this).closest("li").css({'margin-right':0,'clear':'none'}).show();
							}
						});
					}else if(imgNum == 3){
						$el.find("img").css('width',imgS);
						$el.find("img").each(function(index){
							if(index < 2){
								snm.$(this).closest("li").css({'margin-right':'10px','clear':'none'}).show();
							}else{
								snm.$(this).closest("li").css({'margin-right':0,'clear':'none'}).show();
							}
						});
					}else if(imgNum == 4){
						$el.find("img").css('width',imgL);
						$el.find("li").show();
					}else{
						$el.hide();
						return false;
					}
					$el.show();
				},wait);
			}else if(imgNum === 0){
				if(count > confirmNum){
					$el.hide();
					clearInterval(timer);
					return false;
				}
			}else{
				$el.hide();
				clearInterval(timer);
				return false;
			}
		},interval);

	};
	snm.$(function(){
		new p_osa0040002_004_s_0001_01(snm.$(".js-p_osa0040002_004_s_0001_01"));
	});
</script>

